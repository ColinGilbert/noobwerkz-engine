       ac_fn_c_check_header_mongrel "$LINENO" "freetype/freetype.h" "ac_cv_header_freetype_freetype_h" "$ac_includes_default"
if test "x$ac_cv_header_freetype_freetype_h" = xyes; then :
  have_freetype_h='yes'
